<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="BackgroundColor" Color="#FF1E1E1E"/>
    <SolidColorBrush x:Key="ForegroundColor" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="BorderColor" Color="#FF3C3C3C"/>
    <SolidColorBrush x:Key="AccentColor" Color="#2E2E2E"/>
    <SolidColorBrush x:Key="SelectedBackgroundColor" Color="#FF444444"/>

    <SolidColorBrush x:Key="MainColor" Color="Black"/>
    <SolidColorBrush x:Key="MainColorLight" Color="DimGray"/>
    <SolidColorBrush x:Key="MainColorDark" Color="DimGray"/>

    <SolidColorBrush x:Key="BorderDarkMainBrush" Color="#C0C0C0"/>

    <SolidColorBrush x:Key="BackgroundGrayDark" Color="#FFEFEFEF"/>
    <SolidColorBrush x:Key="BackgroundGrayLight" Color="#F5F5F5"/>

    <SolidColorBrush x:Key="ForegroundDisabledBrush" Color="White"/>

    <SolidColorBrush x:Key="SelectedForegroundColor" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="SelectedBorderColor" Color="#222222"/>
    <SolidColorBrush x:Key="HoverBackgroundColor" Color="#666666"/>
    <SolidColorBrush x:Key="HoverForegroundColor" Color="#FFFFFF"/>
    
</ResourceDictionary>
